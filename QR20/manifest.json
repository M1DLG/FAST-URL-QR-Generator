{
  "manifest_version": 3,
  "name": "URL QR Code Generator",
  "version": "2.0",
  "permissions": ["activeTab", "downloads"],
  "background": {
    "service_worker": "popup.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    }
  },
  "icons": {
    "16": "logo.png",
    "48": "logo.png",
    "128": "logo.png"
  }
}
